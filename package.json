{
  "name": "pro_management_test",
  "private": true,
  "version": "1.0.7",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/src/index.js",
    "client": "vite --host",
    "build": "vite build",
    "build:server": "tsc --project server/tsconfig.json",
    "vercel-build": "npm run build && npm run build:server",
    "start": "node server/src/index.js",
    "type-check": "tsc --noEmit",
    "type-check:server": "tsc --project server/tsconfig.json --noEmit",
    "dev:server": "tsx watch server/src/index.ts",
    "start:server": "node dist/server/index.js",
    "lint:typescript": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test:connection": "node scripts/test-connection.js",
    "test:connection:simple": "node scripts/test-connection-simple.js",
    "fix:mongo": "node scripts/fix-mongo-connection.js",
    "fix:mongo:quick": "./scripts/quick-mongo-fix.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --config jest.config.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --config jest.config.js",
    "test:backend": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=server --config jest.config.js",
    "test:frontend": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=src --config jest.config.js",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=unit --config jest.config.js",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=integration --config jest.config.js",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=e2e --config jest.config.js",
    "test:comprehensive": "node scripts/run-comprehensive-tests.js",
    "test:performance": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=performance --config jest.config.js",
    "test:security": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=security --config jest.config.js",
    "test:accessibility": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=accessibility --config jest.config.js",
    "test:all": "npm run test:comprehensive"
  },
